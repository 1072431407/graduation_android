<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.ApplyActivity"></activity>
        <activity android:name=".activitys.LoginActivity"></activity>
        <activity android:name=".activitys.RegisterActivity"></activity>
        <activity android:name=".activitys.ForgetPassActivity"></activity>
        <activity android:name=".activitys.MineListActivity_0"></activity>
        <activity android:name=".activitys.MineListActivity_1"></activity>
        <activity android:name=".activitys.MineListActivity_2"></activity>
        <activity android:name=".activitys.MineListActivity_3"></activity>
        <activity android:name=".activitys.MineListActivity_4"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>